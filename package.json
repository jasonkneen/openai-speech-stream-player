{
  "name": "openai-speech-stream-player",
  "version": "1.0.9",
  "description": "It's a player for play SSE streaming chunk from OpenAI audio speech API.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "homepage": "https://github.com/kvsur/openai-speech-stream-player",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "start": "http-server ./"
  },
  "keywords": [
    "OpenAI Audio",
    "Speech API",
    "stream",
    "player",
    "SSE",
    "stream chunk player"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.9.3",
    "http-server": "14.1.1"
  },
  "files": ["lib/", "package.json", "example/", "README.md", "LICENSE", "tsconfig.json", "tsconfig.cjs.json"]
}
